{
  "name": "gr3-be",
  "module": "src/index.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc && tsc-alias",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "format": "prettier --write \"src/**/*.ts\" && npx prisma format",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prisma:seed": "npx prisma db seed",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:docker": "npx prisma migrate reset --force",
    "prisma:generate": "npx prisma generate --generator client",
    "prisma:prod": "npx prisma migrate deploy",
    "prod": "node dist/index.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/on-finished": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "chalk": "^4.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "on-finished": "^2.4.1",
    "prettier": "^3.0.1",
    "prisma": "^5.20.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.8.0",
    "typescript": "^5.0.0",
    "winston": "^3.13.0"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma",
    "seed": "ts-node --files src/database/seeds/index.ts"
  }
}
